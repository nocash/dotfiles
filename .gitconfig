[alias]
	co = checkout
	ci = commit
	cp = cherry-pick
	st = status
	d = diff
	l = log --graph --relative-date  --pretty=format:'%C(yellow)%h%Creset %C(blue)%d%Creset %s %C(black)(%cd by %cn)%Creset'
	ll = log --graph --relative-date  --pretty=format:'%C(yellow)%h%Creset %C(blue)%d%Creset %s %C(black)(%cd by %cn)%Creset' --all
	fa = fetch --all
	mirror = push mirror --mirror
[color]
	ui = auto
[core]
	excludesfile = ~/.gitignore
	autocrlf = input
	editor = vim
	eol = lf
[merge]
	tool = vimdiff
